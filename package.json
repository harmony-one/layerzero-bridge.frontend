{
  "name": "ethhmy-bridge.frontend",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve --config config/webpack.config.js",
    "dev": "cross-env MODE=development NODE_ENV=mainnet webpack serve --config config/webpack.config.js --progress --color --open",
    "build": "node scripts/build.js --stats",
    "test": "jest --verbose",
    "test:watch": "jest --watch",
    "test:debug": "jest --config=jest.config.debug.js",
    "version": "echo $npm_package_version"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/harmony-one/ethhmy-bridge.frontend"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-brands-svg-icons": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@harmony-js/core": "^0.1.57",
    "@ibm/mobx-react-router": "^6.1.0",
    "@metamask/detect-provider": "^2.0.0",
    "async-validator": "^4.2.5",
    "axios": "^1.8.3",
    "bignumber.js": "^9.1.2",
    "classnames": "^2.5.1",
    "core-decorators": "^0.20.0",
    "grommet": "^2.45.1",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "mobx": "6.13.6",
    "mobx-react": "^9.2.0",
    "mobx-react-router": "^6.1.0",
    "moment": "^2.30.1",
    "polished": "^4.3.1",
    "qs": "^6.14.0",
    "rc-table": "^7.50.4",
    "rc-tooltip": "^6.4.0",
    "react": "^18.0.0",
    "react-cookie-banner": "^4.1.0",
    "react-dom": "^18.0.0",
    "react-outside-click-handler": "^1.3.0",
    "react-responsive": "^10.0.1",
    "react-router": "^6.14.2",
    "react-router-dom": "^6.14.2",
    "react-select": "^5.10.1",
    "react-switch": "^7.1.0",
    "superagent": "^10.2.0",
    "tslib": "^2.8.1",
    "web3": "^4.16.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.16",
    "@types/react": "^18.0.10",
    "@types/react-dom": "^18.0.4",
    "@types/react-outside-click-handler": "^1.3.4",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.34",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^13.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "dotenv": "16.4.7",
    "dotenv-expand": "12.0.1",
    "eslint": "^9.22.0",
    "fs-extra": "^11.3.0",
    "html-webpack-plugin": "^5.6.3",
    "jest": "^29.7.0",
    "lint-staged": "^9.5.0",
    "prettier": "^3.5.3",
    "raf": "^3.4.1",
    "react-dev-utils": "^12.0.1",
    "resolve": "^1.22.10",
    "style-loader": "^4.0.0",
    "styled-components": "^6.1.15",
    "stylus": "0.64.0",
    "stylus-loader": "8.1.1",
    "terser-webpack-plugin": "^5.3.14",
    "ts-jest": "^29.2.6",
    "ts-loader": "^9.5.2",
    "typescript": "5.8.2",
    "url-loader": "4.1.1",
    "webpack": "^5.98.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0",
    "webpack-manifest-plugin": "^5.0.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!**/*.d.ts"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/**/*.(spec|test).(j|t)s?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "<rootDir>/config/jest/typescriptTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|ts|tsx)$"
    ],
    "moduleFileExtensions": [
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "web.js",
      "js",
      "web.jsx",
      "jsx",
      "json",
      "node",
      "mjs"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "./tsconfig.test.json"
      }
    },
    "moduleNameMapper": {
      "^src(.*)$": "<rootDir>/src/$1",
      "^components(.*)$": "<rootDir>/src/components/$1",
      "^constants(.*)$": "<rootDir>/src/constants/$1",
      "^contexts(.*)$": "<rootDir>/src/contexts/$1",
      "^models(.*)$": "<rootDir>/src/models/$1",
      "^modalPages(.*)$": "<rootDir>/src/modalPages/$1",
      "^pages(.*)$": "<rootDir>/src/pages/$1",
      "^stores(.*)$": "<rootDir>/src/stores/$1",
      "^services(.*)$": "<rootDir>/src/services/$1",
      "^themes(.*)$": "<rootDir>/src/themes/$1",
      "^ui(.*)$": "<rootDir>/src/ui/$1",
      "^utils(.*)$": "<rootDir>/src/utils/$1"
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "proxy": "http://localhost:3001",
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint",
      "git add"
    ],
    "*.{css,json,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
